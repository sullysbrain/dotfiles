    "editor.cursorBlinking": "blink",
    "editor.hideCursorInOverviewRuler": true,
    "editor.occurrencesHighlight": "off",
    "editor.roundedSelection": false,
    "editor.selectionHighlight": true,
    "editor.scrollBeyondLastLine": false,
    "explorer.decorations.colors": true,
    "explorer.openEditors.visible": 0,
    "workbench.editor.showIcons": true,
    "workbench.startupEditor": "none",
    "workbench.tree.renderIndentGuides": "true",
    "editor.matchBrackets": "never",
    "subtleBrackets.style": {
        "borderWidth": "1px",
        "borderColor": "#528BFF"
    },
    "notebook.lineNumbers": "on",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "omnisharp.useModernNet": false,
    "breadcrumbs.filePath": "last",
    "editor.parameterHints.enabled": false,
    "terminal.integrated.autoReplies": {
        "terminal.external.osxExec": "iTerm.app",
        "terminal.integrated.fontFamily": "Meslo LG M for Powerline",
        "terminal.integrated.shell.osx": "zsh",
    },
    "editor.inlineSuggest.enabled": true,
    "git.enableSmartCommit": true,
    "editor.accessibilityPageSize": 12,
    "editor.fontSize": 11,
    "window.title": "${dirty} ${activeEditorLong}${separator}${rootName}",
    "window.titleSeparator": " |  ",
    "git.confirmSync": false,
    "C_Cpp.autocompleteAddParentheses": true,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "arduino.useArduinoCli": true,
    "editor.fontFamily": "'DejaVu Sans Mono', 'mononoki', 'MesloLGS NF', Menlo, Monaco, 'Courier New', monospace",
    "workbench.colorTheme": "Nord",
    "editor.minimap.autohide": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editor.alwaysShowEditorActions": true,
    "C_Cpp.default.compilerPath": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin",
    "python.terminal.activateEnvironment": false,
}